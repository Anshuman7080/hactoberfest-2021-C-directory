#include<stdio.h>
#include<conio.h>
void main()
{
    int a[50],i,n,x,f=0;
    printf("Enter size of array \n");
    scanf("%d",&n);
    printf("Enter elements \n");
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("Enter element you want to search \n");
    scanf("%d",&x);
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            printf("%d",i+1);
            f=1;
            break;
        }
    }
    if(f==0)
    {
        printf("No. doesn't exist");
    }
}
